<svg width="120" height="120" viewBox="0 0 120 120" xmlns="http://www.w3.org/2000/svg">
  <!-- Professional Security Shield Logo -->
  <defs>
    <linearGradient id="shieldGradient" x1="0%" y1="0%" x2="0%" y2="100%">
      <stop offset="0%" style="stop-color:#D4AF37;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#B8960C;stop-opacity:1" />
    </linearGradient>
    <filter id="glow">
      <feGaussianBlur stdDeviation="2" result="coloredBlur"/>
      <feMerge>
        <feMergeNode in="coloredBlur"/>
        <feMergeNode in="SourceGraphic"/>
      </feMerge>
    </filter>
  </defs>

  <!-- Outer glow -->
  <path d="M60 10 L95 25 L95 55 Q95 85 60 110 Q25 85 25 55 L25 25 Z"
        fill="none" stroke="#D4AF37" stroke-width="2" opacity="0.3" filter="url(#glow)"/>

  <!-- Main shield -->
  <path d="M60 15 L90 28 L90 55 Q90 82 60 105 Q30 82 30 55 L30 28 Z"
        fill="url(#shieldGradient)" stroke="#C0C0C0" stroke-width="2"/>

  <!-- Inner shield detail -->
  <path d="M60 25 L80 35 L80 55 Q80 75 60 90 Q40 75 40 55 L40 35 Z"
        fill="rgba(0,0,0,0.2)"/>

  <!-- Checkmark symbol -->
  <path d="M48 55 L56 65 L72 45"
        stroke="#fff" stroke-width="6" stroke-linecap="round" stroke-linejoin="round"
        fill="none" filter="url(#glow)"/>

  <!-- Company initials placeholder -->
  <text x="60" y="100" font-family="Arial, sans-serif" font-size="14" font-weight="bold"
        fill="#D4AF37" text-anchor="middle">ABC</text>
</svg>
